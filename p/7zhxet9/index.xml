&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">AL ALB Albania
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">... 省略 ...
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">WS WSM Samoa
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">YE YEM Yemen
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">YT MYT Mayotte
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">ZA ZAF South Africa
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">ZM ZMB Zambia
&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl">ZW ZWE Zimbabwe&lt;/span>&lt;/span>&lt;/code>&lt;/pre>&lt;/div>&lt;/div>
&lt;/figure>

